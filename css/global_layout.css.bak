#header .headWrap .headRight li.school a:hover {width:80px; float:right; text-align:right;  height:55px;}
#header .headWrap .headRight li.online { float:left; text-align:left; font-size:20px; color:#333; margin-right:80px; height:55px; font-weight:bold}
#header .headWrap .headRight li.online_first { float:left; text-align:left; font-size:20px; color:#333; padding-left:30px; margin: 0 79px 0 0; height:55px; font-weight:bold}
#header .headWrap .headRight li.online_02 { float:left; text-align:left; font-size:20px; color:#333; width:130px; height:55px; font-weight:bold}

#header .headWrap .headRight li.post { float:left; text-align:left; font-size:20px; color:#333; margin-right:77px; height:55px; font-weight:bold;}
#header .headWrap .headRight li.post_first { float:left; text-align:left; font-size:20px; color:#333; margin: 0 77px 0 30px; height:55px; font-weight:bold;}
#header .headWrap .headRight li.post_02 { float:right; text-align:right; font-size:20px; color:#333; height:55px; font-weight:bold;  margin-right:0; }
#header .headWrap .headRight li .post_selecton,
#header .headWrap .headRight li.post a:hover , 
#header .headWrap .headRight li.post_first a:hover,  
#header .headWrap .headRight li.post_02 a:hover{ float:left; text-align:left; font-size:20px; color:#3dbbd1;  border-bottom:3px solid #3dbbd1; height:55px;}
