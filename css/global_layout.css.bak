/********************************************
	FOOTER
********************************************/
.footerWrap {position:fixed width:100%; background:#646464; text-align:center; clear:both}
.footerWrap a {display:block;font-size:13px;font-weight:bold; color:#b7b7b7; line-height:140%}
.footerWrap a:hover {text-decoration:none;}
.footerWrap ul:after {content:".";display:block;height:0px;clear:both;visibility:hidden;}
.footerWrap li:first-child {background:none;}
.footerWrap li {float:left;}
.footerWrap footer {position:relative; margin:0 auto; padding:27px 0; width:1200px; letter-spacing:-1px;/* max-width:1200px; */ display:inline-block; text-align:left}
.footerWrap footer .ftLogo {width:220px; float:left; padding-top:5px;}

.footerWrap footer .footerLink {float:left; }
.footerLink ul {overflow:hidden;}
.footerLink li {background:url('../images/common/bg_defaultLine.png') no-repeat left center;margin-left:20px;padding-left:20px; font-weight:bold;  line-height:140%}
.footerLink li:first-child {margin-left:0;padding-left:0;}
.footerWrap address {display:block; margin-bottom:5px;font-style:normal; font-size:13px; color:#b7b7b7;}
.footerWrap address span {display:inline-block;}

.footerWrap footer .asideWrap { position:relative; border:1px solid #000; display:inline-block; float:right}
.familySite {position:relative;width:135px;}
.familySite .tit {display:block; background-color:#2b2b2b;}
.familySite .tit a {background:url('../images/common/icon_ArrBottom.png') no-repeat right center;padding:5px 25px 5px 10px;}
.familySite ul {display:none;position:absolute;left:0;right:0;bottom:29px;background-color:#2b2b2b;border:1px solid #5d5d5d;}
.familySite li {float:none;}
.familySite li a {padding:5px 10px;}
.familySite.on .tit a {background:url('../images/common/icon_ArrBottom.png') no-repeat right center;}
